{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title" : "User Profile",
    "type" : "object",
    "required": ["name", "email","age"],
    "properties": {
        "name" : {
            "type" : "string",
            "minLength" : 1
        },
        "email" : {
            "type" : "string",
            "format": "email"
        },
        "age" : {
            "type" : "integer"
        }

    },
    "additionalProperties": false
}